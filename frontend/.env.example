# Mobius Luminous Dashboard - Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# REQUIRED: Supabase Configuration
# ===========================================

# Your Supabase project URL
VITE_SUPABASE_URL=https://your-project.supabase.co

# Your Supabase anonymous (public) key
# This is safe to expose in the frontend
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ===========================================
# OPTIONAL: API Configuration
# ===========================================

# Backend API base URL (defaults to relative path if not set)
# VITE_API_BASE_URL=http://localhost:8000

# ===========================================
# OPTIONAL: Feature Flags
# ===========================================

# Enable debug mode (shows additional logging)
# VITE_DEBUG_MODE=false

# Enable mock data for development without backend
# VITE_USE_MOCK_DATA=false

# ===========================================
# OPTIONAL: Analytics & Monitoring
# ===========================================

# Error tracking service DSN (e.g., Sentry)
# VITE_SENTRY_DSN=

# Analytics tracking ID
# VITE_ANALYTICS_ID=
